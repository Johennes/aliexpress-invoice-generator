{
  "manifest_version": 2,
  "name": "AliExpress Invoice Generator",
  "version": "0.1.0",
  "description": "Generates print-ready invoices from AliExpress order detail websites",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://trade.aliexpress.com/order_detail*"],
      "js": ["jquery-3.4.1.min.js", "handlebars-4.1.2.min.js", "content.js"]
    }
  ]
}
